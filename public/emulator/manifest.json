{
  "id": "spy",
  "name": "Spy",
  "entryPoint": "index.html",
  "icon": "icon.png",
  "minSdk": "2.1.1",
  "description": "Blend in with the rest of the group",
  "version": "1.0.0",
  "category": "strategy",
  "beemi": {
    "sdkVersion": "2.1.2",
    "core": {
      "required": true
    },
    "streams": {
      "required": true
    },
    "multiplayer-p2p": {
      "required": true
    },
    "user": {
      "required": true,
      "config": {
        "enableProfileSync": true,
        "cacheUserData": true
      }
    },
    "app-bridge": {
      "required": true,
      "config": {
        "enableAuthentication": true,
        "enableNotifications": true
      }
    }
  },
  "development": {
    "framework": "generic",
    "command": "npm run dev:emulator",
    "url": "http://localhost:8080",
    "port": 8080,
    "emulatorPath": "/emulator",
    "emulatorDir": "./emulator",
    "replaceEmulatorFiles": false,
    "postCreate": "npm install"
  },
  "production": {
    "entryPoint": "index.html"
  }
}